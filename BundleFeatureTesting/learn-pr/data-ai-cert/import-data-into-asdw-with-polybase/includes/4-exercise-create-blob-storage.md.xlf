        </trans-unit><trans-unit id="106" translate="yes" xml:space="preserve">
          <source>Select <bpt id="p1">**</bpt>Create a Resource<ept id="p1">**</ept> in the left sidebar.</source>
        </trans-unit><trans-unit id="107" translate="yes" xml:space="preserve">
          <source>Select <bpt id="p1">**</bpt>Storage<ept id="p1">**</ept><ph id="ph1"> &gt; </ph><bpt id="p2">**</bpt>Storage account<ept id="p2">**</ept>.</source>
