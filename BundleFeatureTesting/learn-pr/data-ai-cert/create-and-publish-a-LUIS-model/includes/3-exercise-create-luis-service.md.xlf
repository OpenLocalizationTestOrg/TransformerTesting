          <source>For <bpt id="p1">**</bpt>Location<ept id="p1">**</ept>, choose the one nearest you.</source>
        </trans-unit><trans-unit id="119" translate="yes" xml:space="preserve">
          <source>For <bpt id="p1">**</bpt>Pricing tier<ept id="p1">**</ept>, select <bpt id="p2">**</bpt>S0<ept id="p2">**</ept>.</source>
        </trans-unit><trans-unit id="120" translate="yes" xml:space="preserve">
          <source>Create a new resource group named <bpt id="p1">**</bpt>LearnRG<ept id="p1">**</ept>.</source>
